<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<Version>1.0.0</Version>
	<AssemblyVersion>1.0.0.0</AssemblyVersion>
	<FileVersion>1.0.0.0</FileVersion>
	<Authors>wanlitao</Authors>
	<Description>HEF Extensions for Serilog sink of Grafana Loki</Description>
	<Company>GreatBillows</Company>
	<Copyright>Copyright © GreatBillows 2023</Copyright>
	<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	<PackageProjectUrl>https://github.com/HEF-Sharp/HEF.Extensions.Serilog</PackageProjectUrl>
	<PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0.html</PackageLicenseUrl>
	<RepositoryUrl>https://github.com/HEF-Sharp/HEF.Extensions.Serilog.git</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Serilog.Settings.Configuration" Version="3.3.0" />
    <PackageReference Include="Serilog.Sinks.Grafana.Loki" Version="8.1.0" />
  </ItemGroup>

</Project>
